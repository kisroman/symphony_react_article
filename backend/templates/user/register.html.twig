{% extends 'base.html.twig' %}

{% block title %}Create user{% endblock %}

{% block body %}
    <h1>Create user</h1>

    {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
        {{ form_row(form.password) }}
        <button type="submit">Create</button>
    {{ form_end(form) }}
{% endblock %}
